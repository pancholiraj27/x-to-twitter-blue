{
  "name": "X To Twitter Blue",
  "version": "0.0.2",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "host_permissions": ["https://twitter.com/*"],
  "icons": {
    "128": "logo.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["main.js"]
    }
  ]
}
