{
    "name": "X To Twitter Blue" ,
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
      ],
      "host_permissions": [
        "https://twitter.com/*"
      ],
      "icons": {
        "128":"logo.png"
      },
      "content_scripts": [
        {
          "matches": ["https://twitter.com/*"],
          "js": ["main.js"]
        }
      ]
}
